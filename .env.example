# MediAssist AI Environment Variables

# Database
DATABASE_URL=postgresql+asyncpg://mediassist:password@localhost:5433/mediassist

# MongoDB (Local or Atlas)
# Local: mongodb://localhost:27017
# Atlas: mongodb+srv://username:password@cluster.mongodb.net
MONGODB_URI=mongodb://localhost:27017
MONGODB_DB=mediassist

# Redis
REDIS_URL=redis://localhost:6380/0

# Security
JWT_SECRET=your-secret-key-change-in-production
JWT_ALGORITHM=RS256

# OpenAI
OPENAI_API_KEY=your-openai-api-key

# ChromaDB
CHROMA_HOST=localhost
CHROMA_PORT=8005

# MinIO
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_BUCKET=reports

# ML Models
SYMPTOM_MODEL_PATH=./models/symptom_classifier/weights
IMAGE_MODEL_PATH=./models/image_analyzer/weights

# CORS
CORS_ORIGINS=http://localhost:3000

# Debug
DEBUG=true
